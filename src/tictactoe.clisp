(defun initial-game ()
    ())

(defun game-status (game)
    (if (evenp (length game)) 
        'x-to-play
        'o-to-play))

(defun move (game row col)
    (labels ((other-player (game)
        (if (eql 'x-to-play (game-status game))
            'o
            'x)))
        (if (assoc (cons row col) game)
            game
            (cons (cons (cons row col) (other-player game)) game))))
